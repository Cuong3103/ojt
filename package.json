{
  "name": "nhom-2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.(ts|tsx|js|jsx)\"",
    "test": "jest",
    "test:watch": "npx jest --watchAll",
    "test:ci": "npx jest --config ./jest.config.ts --ci --watchAll=false --forceExit --passWithNoTests",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky",
    "docker:prepare:db": "docker pull doanvinhphu321/postgres",
    "docker:prepare:backend": "docker pull doanvinhphu321/nhom_2-fams",
    "docker:start:db": "docker run -d --name spring-app-container -e SPRING_DATASOURCE_URL=jdbc:postgresql://postgres-container:5432/fams -e SPRING_DATASOURCE_USERNAME=postgres -e SPRING_DATASOURCE_PASSWORD=sa -p 8080:8080 doanvinhphu321/nhom_2-fams:latest",
    "docker:start:backend": "docker run -d --name spring-app-container --link postgres-container -e SPRING_DATASOURCE_URL=jdbc:postgresql://postgres-container:5432/fams -e SPRING_DATASOURCE_USERNAME=postgres -e SPRING_DATASOURCE_PASSWORD=sa -p 8080:8080 doanvinhphu321/nhom_2-fams:latest"
  },
